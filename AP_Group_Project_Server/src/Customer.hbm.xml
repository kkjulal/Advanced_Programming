<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernatee/Hibernate Configuration DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Customer" table="customer" >
		<id column="id" name="customer_id" type="java.lang.String"/>
		<property name="firstName" column="first_name" type="java.lang.String"/>
		<property name="lastName" column="last_name" type="java.lang.String"/>
		<property name="address" column="address" type="java.lang.String"/>
		<property name="telephone" column="telephone" type="java.lang.String"/>
		<property name="email" column="email" type="java.lang.String"/>
		<property name="balance" column="balance" type="java.lang.String"/>
		<property name="password" column="password" type="java.lang.String"/>		
	</class>
</hibernate-mapping>